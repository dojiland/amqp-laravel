{
    "name": "dojiland/amqp-laravel",
    "description": "PHP RabbitMQ client for Laravel && Lumen",
    "keywords": ["Laravel", "Lumen", "AMQP", "RabbitMQ"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.3",
        "php-amqplib/php-amqplib": "^2.12",
        "psr/log": "^1.1",
        "evenement/evenement": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dojiland\\Amqp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dojiland\\Amqp\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Dojiland\\Amqp\\AmqpServiceProvider"
            ]
        }
    }
}
